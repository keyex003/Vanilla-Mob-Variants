{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:chicken",
      "materials": {
        "default": "chicken",
        "legs": "chicken_legs"
      },
      "textures": {
        "default": "textures/entity/chicken",
        "default2": "textures/entity/chicken/chicken2",
        "default3": "textures/entity/chicken/chicken3",
        "default4": "textures/entity/chicken/chicken4",
		"default5": "textures/entity/chicken/chicken5",
        "default6": "textures/entity/chicken/chicken6",
        "default_b": "textures/entity/chicken/chicken_b"
      },
      "geometry": {
        "default": "geometry.chicken"
      },
      "animations": {
        "move": "animation.chicken.move",
        "general": "animation.chicken.general.v1.0",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.chicken.baby_transform"
      },
      "scripts": {
        "initialize": [
           "variable.Xvar = Math.random(0,99);"
        ],
        "animate": [
          "general",
          { "move": "query.modified_move_speed" },
          "look_at_target",
          { "baby_transform": "query.is_baby" }
        ]
      },
      "render_controllers": [ "controller.render.chicken" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 0
      }
    }
  }
}
