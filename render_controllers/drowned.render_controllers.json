{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.drowned": {
      "arrays": {
        "textures": {
          "Array.skins": [ 
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
			"Texture.default4",
            "Texture.default5",
            "Texture.default6",
            "Texture.default"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Array.skins[variable.Xvar]" ],
	  "overlay_color" : {
          "r": "variable.Xvar>50 ? 0.6 : -1.0",
          "g": "variable.Xvar>50 ? 0.8 : -0.8",
          "b": "variable.Xvar>50 ? 0.8 : -0.8",
          "a": "math.abs(variable.Xvar-50)*0.003"
      }
    }
  }
}
