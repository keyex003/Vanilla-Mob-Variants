{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.creeper": {
	  "arrays": {
        "textures": {
          "Array.skins": [ 
            "Texture.default",
            "Texture.default2",
			"Texture.default3",
			"Texture.default4",
			"Texture.default5",
			"Texture.default6",
			"Texture.default7",
			"Texture.default8",
			"Texture.default9",
			"Texture.default10",
			"Texture.default11",
			"Texture.default12",
			"Texture.default13",
			"Texture.default14",
			"Texture.default"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Array.skins[variable.Xvar]" ],
      "overlay_color": {
          "g": "((variable.flash == 0.0) || (query.swelling_dir < 0.0)) ? (variable.Xvar>50 ? 1.0 : -0.95) : 1.0",
          "r": "((variable.flash == 0.0) || (query.swelling_dir < 0.0)) ? (variable.Xvar>50 ? 0.95 : -1.0) : 1.0",
          "b": "((variable.flash == 0.0) || (query.swelling_dir < 0.0)) ? (variable.Xvar>50 ? 0.95 : -1.0) : 1.0",
          "a": "((variable.flash == 0.0) || (query.swelling_dir < 0.0)) ? (variable.Xvar*0.002) : 1.0"
      }
    }
  }
}
