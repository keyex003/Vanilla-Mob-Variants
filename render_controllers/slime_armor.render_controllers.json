{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.slime_armor": {
	  "arrays": {
        "textures": {
          "Array.skins": [ 
            "Texture.default",
            "Texture.default2",
			"Texture.default3",
			"Texture.default4",
			"Texture.default5",
			"Texture.default6",
			"Texture.default7",
			"Texture.default8",
			"Texture.default9",
			"Texture.default10",
			"Texture.default"
          ]
        }
      },
      "geometry": "Geometry.armor",
      "materials": [ { "*": "Material.outer" } ],
      "textures": [ "Array.skins[variable.Xvar]" ],
	  "overlay_color" : {
          "g": "variable.Xvar>50 ? 1.0 : -0.95",
          "r": "variable.Xvar>50 ? 0.95 : -1.0",
          "b": "variable.Xvar>50 ? 0.95 : -1.0",
          "a": "variable.Xvar*0.002"
        }
    }
  }
}
